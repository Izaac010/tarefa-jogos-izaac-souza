programa {
  funcao inicio() {
   caracter select
   escreva("Digite uma opção: ")
   leia(select)

   escolha(select){
    caso 'Z':
    {
      escreva("Zíngaro")
      pare
    }
    caso 'X':
    {
      escreva("Xarope Estelar")
      pare
    }
    caso 'K':
    {
      escreva("Kryptonita")
      pare
      }
      caso contrario:
      {
        escreva ("Opção invalida")
      }
    }
   }  
  }

