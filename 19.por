programa {
  funcao inicio() {
  real altura
  escreva("Digite a altura das nuvens: ")
  leia(altura)

   se(altura<2000){
    escreva("As nuvens são baixas")
   }
   senao se(altura<=6000){
    escreva("As nuvens são medias")
   }
   senao se(altura>6000){
    escreva("As nuvens são altas")
   }

   }  
  }
}
